{
  "tasks": {
    "install": "deno install --allow-import",
    "update": "deno outdated --update --latest",
    "lint": "deno lint",
    "test": "deno test src/main_test.ts --allow-net --allow-import",
    "cache": "deno cache src/main.ts --allow-import",
    "dev": "deno run src/main.ts --allow-import --allow-net --allow-read --allow-write --allow-env --watch --inspect",
    "run": "deno run src/main.ts --allow-import --allow-net --allow-read --allow-write --allow-env",
    "start": "deno task run",
    "compile": "deno compile src/main.ts --allow-import --allow-net --allow-read --allow-write --allow-env --output previewsynth",
    "compile-windows-x86_64": "deno compile src/main.ts --allow-import --allow-net --allow-read --allow-write --allow-env --output previewsynth --target x86_64-pc-windows-msvc",
    "compile-macos-x86_64": "deno compile src/main.ts --allow-import --allow-net --allow-read --allow-write --allow-env --output previewsynth --target x86_64-apple-darwin",
    "compile-macos-aarch64": "deno compile src/main.ts --allow-import --allow-net --allow-read --allow-write --allow-env --output previewsynth --target aarch64-apple-darwin",
    "compile-linux-x86_64": "deno compile src/main.ts --allow-import --allow-net --allow-read --allow-write --allow-env --output previewsynth --target x86_64-unknown-linux-gnu",
    "compile-linux-aarch64": "deno compile src/main.ts --allow-import --allow-net --allow-read --allow-write --allow-env --output previewsynth --target aarch64-unknown-linux-gnu"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.13",
    "@std/dotenv": "jsr:@std/dotenv@0.225.4",
    "@std/log": "jsr:@std/log@^0.224.14",
    "@std/yaml": "jsr:@std/yaml@1.0.6",
    "x/sleep": "https://deno.land/x/sleep@v1.3.0/mod.ts",
    "x/grammy": "https://deno.land/x/grammy@v1.36.1/mod.ts",
    "x/grammy_types/manage": "https://deno.land/x/grammy_types@v3.20.0/manage.ts",
    "x/grammy_types/message": "https://deno.land/x/grammy_types@v3.20.0/message.ts",
    "x/grammy_types/context": "https://deno.land/x/grammy_types@v3.20.0/context.ts",
    "x/grammy_types/inline": "https://deno.land/x/grammy_types@v3.20.0/inline.ts"
  },
  "unstable": [
    "temporal"
  ]
}