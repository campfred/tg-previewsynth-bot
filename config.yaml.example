  - name: FurAffinity
    origins:
      - https://furaffinity.net
    destination: https://xfuraffinity.net
  - name: FurTrack
    origins:
      - https://furtrack.com/p/
    origins_regex:
      - ^https:\/\/(www\.)?furtrack\.com\/(\w+\/)?(\w+[:/])?\w+(\(\w+\))?\/
    destination: https://furtrack.owo.lgbt/p/
  - name: Reddit
    origins:
      - https://reddit.com
    destination: https://vxreddit.com
  - name: Bluesky
    origins:
      - https://bsky.app
    destination: https://fxbsky.app
  - name: Twitter
    origins:
      - https://twitter.com
      - https://x.com
    destination: https://fxtwitter.com
  - name: Instagram
    origins:
      - https://instagram.com
    destination: https://ddinstagram.com
  - name: TikTok
    origins:
      - https://tiktok.com
    destination: https://tfxktok.com
apis:
  odesli:
    enabled: true
    # api_key: *** # Not using an API key grants you only 10 reqs. per minute
